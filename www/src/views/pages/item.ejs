<script type="text/javascript">

    $("#Tabs").tabs();


    $("#UserSalesTabHeader").addClass("StandardTabGrayActive");
    $("#UserSalesTab").show();


    var commentsLoaded = false;

    $("#UserSalesLink").click(function () {
        $(".ItemTabs").removeClass("StandardTabGrayActive");
        $(".ItemTabs").addClass("StandardTabGray");
        $("#UserSalesTabHeader").addClass("StandardTabGrayActive");
        $("#ScriptReviewTab").hide();
    });

    $("#ScriptReviewLink").click(function () {
        $(".ItemTabs").removeClass("StandardTabGrayActive");
        $(".ItemTabs").addClass("StandardTabGray");
        $("#ScriptReviewTabHeader").addClass("StandardTabGrayActive");
        $("#ScriptReviewTab").show();
    });

    $("#CommentaryLink").click(function () {
        $(".ItemTabs").removeClass("StandardTabGrayActive");
        $(".ItemTabs").addClass("StandardTabGray");
        $("#CommentaryTabHeader").addClass("StandardTabGrayActive");
        $("#ScriptReviewTab").hide();
        $("#CommentaryTab").show()
        if (!commentsLoaded) {
            __doPostBack('ctl00$cphRoblox$CommentsPane$LoadCommentsButton', '');
            commentsLoaded = true;
        }
        return false;
    });


    function confirmDelete() {
        return confirm("Are you sure you want to permanently DELETE this item from your inventory?");
    }

    function confirmSubmit() {
        var resp = confirm("This will add a new badge giver model to your inventory. Are you sure you want to do this?");
        if (resp) {
            window.location.href = $('#ctl00_cphRoblox_btnSubmit').attr('href');
        }
    }

    modalProperties = { escClose: true, opacity: 80, overlayCss: { backgroundColor: "#000" } };

    // Code for Modal Popups


    function ModalClose(popup) {
        $.modal.close('.' + popup);
    }
    function ModalOpen(popup) {
        $(popup).modal(modalProperties);
    }

    function UpdateSets(assetSetItemId, updateAllSets) {
        $.get(
            "/Sets/SetHandler.ashx?rqtype=getnewestversion&assetSetItemId=" + assetSetItemId + (updateAllSets ? "&allsets=true" : ""),
            function () {
                $('#ctl00_cphRoblox_UpdateSet').hide("slow");
                if (updateAllSets) {
                    $('#ctl00_cphRoblox_updateSetsDiv').slideToggle("slow");
                }
            });
    }

    function getItem(view) {
        switch (view) {
            case "PurchaseFree":
                $('#ctl00_cphRoblox_Button2').click();
                break;
            case "PurchaseRobux":
                $('#ctl00_cphRoblox_Button3').click();
                break;
            case "UAPurchaseRobux":
                $('#ctl00_cphRoblox_Button4').click();
                break;
            case "PurchaseTickets":
                $('#ctl00_cphRoblox_Button5').click();
                break;
        }
    }
</script>

