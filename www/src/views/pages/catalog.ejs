        <div class="Assets">
            <div id="ctl00_cphRoblox_rbxCatalog_HeaderPagerPanel" class="HeaderPager" style="display: none;">

                <span id="ctl00_cphRoblox_rbxCatalog_HeaderPagerLabel">Page <%= page.currentPage %> of
                        <%= (page.cursor.next ? '100+' : page.currentPage) %></span>
                <a id=" ctl00_cphRoblox_rbxCatalog_HeaderPagerHyperLink_Next" href="Catalog.aspx?m=Featured&amp;c=8&amp;t=AllTime&amp;d=All&amp;q=&amp;p=2&amp;mn=-9223372036854775808&amp;mx=9223372036854775807">
                    Next
                    <span class="NavigationIndicators">&gt;&gt;</span></a>
            </div>
            <div class="StandardBoxHeader">
                <span id="ctl00_cphRoblox_rbxCatalog_AssetsDisplaySetLabel">
                    <%= page.header %>
                </span>
            </div>
            <div class="StandardBox">

                <table id="ctl00_cphRoblox_rbxCatalog_AssetsDataList" cellspacing="0" align="Center" border="0" style="border-collapse:collapse;">
                    <% for (const set of page.items) { %>
                        <tr>
                            <% for (const item of set) { %>
                                <td valign="top">
                                    <div class="Asset" style="margin-left:5px;margin-right:5px;">

                                        <div class="AssetThumbnail">
                                            <a id="ctl00_cphRoblox_rbxCatalog_AssetsDataList_ctl00_AssetThumbnailHyperLink" title="<%= item.name %>" href="/Item.aspx?id=<%= item.id %>" style="display:inline-block;height:110px;width:110px;cursor:pointer;"><img src="https://www.roblox.com/thumbs/asset.ashx?width=110&height=110&assetid=<%= item.id %>" height="110" width="110" border="0" onerror="return Roblox.Controls.Image.OnError(this)" alt="Nordic Conqueror" blankurl="http://t2ak.roblox.com/p1-blank-110x110.gif" /></a>
                                            <% if (item.itemRestrictions.includes('LimitedUnique')) { %>
                                                <div style="position:relative;left:-12px;top:-13px;"><img src="/img/LimitedU.png">
                                                </div>
                                                <% }else if (item.itemRestrictions.includes('Limited')) { %>
                                                    <div style="position:relative;left:-22px;top:-13px;"><img src="/img/Limited.png">
                                                    </div>
                                                    <% } %>

                                        </div>
                                        <div class="AssetDetails">
                                            <div class="AssetName"><a id="ctl00_cphRoblox_rbxCatalog_AssetsDataList_ctl00_AssetNameHyperLink" href="/Item.aspx?id=<%= item.id %>">
                                                    <%= item.name %>
                                                </a></div>
                                            <!-- not really possible anymore due to rate limits
                                <div class="AssetLastUpdate">
                                    <span class="Label">Updated:</span>
                                    <span class="Detail"><%= item.updatedAtFormat %></span>
                                </div>
                                -->
                                            <div class="AssetCreator">
                                                <span class="Label">Creator: </span>
                                                <span class="Detail"><a id="ctl00_cphRoblox_rbxCatalog_AssetsDataList_ctl00_CreatorHyperLink" href="<%= (item.creatorType === 'User' ? '/User.aspx?ID=' : '/Group.aspx?ID=') %><%= item.creatorTargetId %>">
                                                        <%= item.creatorName %>
                                                    </a></span>
                                            </div>

                                            <% if (item.itemRestrictions.includes('LimitedUnique') || item.itemRestrictions.includes('Limited')) { %>
                                                <div class="AssetsSold">
                                                    <span class="Label" style="color:#cc0000;">Remaining:</span>
                                                    <span class="Detail">
                                                        <%= numberFormat(item.unitsAvailableForConsumption || 0) %>
                                                    </span>
                                                </div>
                                                <% } %>
                                                    <div class="AssetFavorites">
                                                        <span class="Label">Favorited:</span>
                                                        <span class="Detail">
                                                            <%= numberFormat(item.favoriteCount) %> times
                                                        </span>
                                                    </div>
                                                    <div class="AssetPrice">
                                                        <span class="PriceInRobux">R$:
                                                            <%= numberFormat(item.price || 0) || 'N/A' %>
                                                        </span>
                                                    </div>


                                                    <% if (item.itemRestrictions.includes('LimitedUnique') || item.itemRestrictions.includes('Limited')) { %>
                                                        <% if (typeof item.lowestPrice !=='undefined' ) { %>
                                                            <div class="AssetFavorites">
                                                                <span class="Detail"><i>Now R$<%= numberFormat(item.lowestPrice) %></i></span>
                                                            </div>
                                                            <% } %>
                                                                <% } %>

                                        </div>
                                    </div>
                                </td>
                                <% } %>
                        </tr>
                        <% } %>

                            <tr>
                                <td colspan="5">
                                    <div id="ctl00_cphRoblox_rbxCatalog_AssetsDataList_ctl20_CatalogDescriptionPanel" style="float: left; width: 630px; font-size: 0.9em; padding: 10px; border-top: 1px solid #AAAAAA;">

                                        Avatar Items - ROBLOX has a full virtual goods catalog with avatar items and other
                                        virtual
                                        items. Create a free account on ROBLOX and start collecting <h1 style="display:inline; font-size:100%; font-weight:normal;">virtual avatar items
                                        </h1>,
                                        virtual goods, virtual items, and other gear for your virtual avatars.

                                    </div>
                                </td>
                            </tr>
                </table>

            </div>
            <div id="ctl00_cphRoblox_rbxCatalog_FooterPagerPanel" class="HeaderPager">
                <% if (page.cursor.previous) { %>
                    <a id="ctl00_cphRoblox_rbxCatalog_FooterPagerHyperLink_Previous" href="/Catalog.aspx?m=<%= page.m %>&cursor=<%= page.cursor.previous %>">&lt;&lt; Previous <span class="NavigationIndicators"></span></a>
                    <% } %>
                        <span id="ctl00_cphRoblox_rbxCatalog_FooterPagerLabel">Page <%= page.currentPage %> of
                                <%= (page.cursor.next ? '100+' : page.currentPage) %></span>
                        <a id="ctl00_cphRoblox_rbxCatalog_FooterPagerHyperLink_Next" href="/Catalog.aspx?m=<%= page.m %>&cursor=<%= page.cursor.next %>">Next <span class="NavigationIndicators">&gt;&gt;</span></a>
            </div>
